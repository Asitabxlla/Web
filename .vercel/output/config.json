{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/charactersheet\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/commissions\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/heavyinkstyle\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/lineart\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/portfolio\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/renderstyle\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/traditionalart\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}